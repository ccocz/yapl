fun(): {
    i=1;
    while (i <= 10): {
        if (i == 5): {
            i++;
            continue;
        }
        if (i == 8): {
            break;
        }
        out(i);
        i++;
    }
    out("finished fun");
}

funB(): {
    for (i=0; to 10): {
        if (i == 2): {
           i++;
           continue;
        }
        if (i == 5): {
            return i;
        }
        out(i);
        i++;
    }
}

main(): {
    fun();
    out(funB());
}